<% if session[:mobile] == "Y" %>
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <%= render 'mmonearticle' %>
      </div>
    </div>
  </div>
<% else %>
  <div class="container">
    <div class='row'> 
      <div class='col-sm-offset-0 col-sm-12'>
        <div class="well">
          <%= render 'onearticle' %>
          <%# params[:donotdisplay] = "Y" %>
          <%= render 'article', obj: @articles %>
          <div align="left">
            <%= will_paginate %>
          </div>
        </div>
      </div>
    </div>
  </div>
<% end %>